{
  "author": "igorlogius",
  "homepage_url": "https://github.com/igorlogius",
  "background": {
    "scripts": ["background.js"]
  },
  "description": "Open a set of bookmarks as tabs on browser startup by simply selecting a bookmark folder which contains them",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_area": "navbar",
    "default_popup": "dialog.html"
  },
  "manifest_version": 2,
  "name": "Startup Bookmarks",
  "options_ui": {
    "page": "dialog.html"
  },
  "permissions": ["tabs", "bookmarks", "storage"],
  "version": "1.7.14"
}
